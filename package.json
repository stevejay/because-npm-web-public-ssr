    "material-colors": "^1.2.6",
    "mitt": "^1.1.3",
    "next": "^8.0.3",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-apollo": "^2.4.1",
    "react-delay": "^0.1.0",
