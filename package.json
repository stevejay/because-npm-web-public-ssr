    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/.next/"
    ],
    "moduleNameMapper": {
      "\\.(s?css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
