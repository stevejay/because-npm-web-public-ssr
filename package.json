    "@storybook/addon-options": "^4.1.13",
    "@storybook/addon-viewport": "^4.1.13",
    "@storybook/addons": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "@types/classnames": "^2.2.7",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.6",
