    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-state": "^0.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-react-svg": "^1.0.1",
